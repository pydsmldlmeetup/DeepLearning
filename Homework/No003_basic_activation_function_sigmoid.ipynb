   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import math\n",
    "\n",
    "e = math.e # e means \"Napier's constant\"\n",
    "\n",
    "def sigmoid(x):\n",
    "    y = 1 / (1 + e**-x)\n",
    "    \n",
    "    # sigmoid returns value 0 < value < 1\n",
    "    return y\n",
    "\n",
    "\n",
    "x = np.arange(-10, 10, 0.1)\n",
    "y = sigmoid(x)\n",
    "\n",
    "plt.plot(x, y)\n",
    "plt.show()"
   ]
  }
 ],
